package harry;

class Animal{

private int weight;

public void animalWeight(int wt){
	this.weight = wt;
	System.out.println("Weight of animal is " +weight);

}


}

class Dog extends Animal{
     private int name;
	public void nameOfAnimal(int n){
	 this.name = n;
	 System.out.println("The name of dog is " +name);
		Dog d= new Dog();
		d.nameOfAnimal(34);
		d.animalWeight(32);
}
}

