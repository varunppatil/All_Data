package practice;

public class PrimeNos {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=20;
		int primeCheck=0;
		int count=0;
		int m1 =n/2;
		
		for(int i=2;i<=m1;i++) {
			primeCheck=i;
			for(int j=i;j<m1;j++) {
			if(primeCheck%i==0) {
				count=1;
				System.out.println(primeCheck+" not prime a no");
			}
		}
		}
			if(count==0) {
				System.out.println(primeCheck+" is a prime no");
			}
		}
}


