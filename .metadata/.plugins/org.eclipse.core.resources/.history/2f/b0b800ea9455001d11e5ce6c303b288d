package harry;

public class Vdo29_ArrayPractise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		float [] arr = {2.3f,4.2f,87.45f,7.8f,9.005f};
		float Sum = 0 ;
		for (int i=0; i<=arr.length;i++) {
			Sum = Sum+arr[i];
			
		}
		System.out.println(Sum);
		
	}

}
